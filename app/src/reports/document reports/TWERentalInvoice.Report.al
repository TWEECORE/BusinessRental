    local procedure InitializeShipmentLine()
    var
        RentalShipmentHeader: Record "TWE Rental Shipment Header";
        RentalShipmentBuffer2: Record "TWE Rental Shipment Buffer";
    begin
        if Line.Type = Line.Type::" " then
            exit;

        if Line."Shipment No." <> '' then
            if RentalShipmentHeader.Get(Line."Shipment No.") then
                exit;

        ShipmentLine.GetLinesForRentalInvoiceLine(Line, Header);

        ShipmentLine.Reset();
        ShipmentLine.SetRange("Line No.", Line."Line No.");
        if ShipmentLine.FindFirst() then begin
            RentalShipmentBuffer2 := ShipmentLine;
            if not DisplayShipmentInformationBoolean then
                if ShipmentLine.Next() = 0 then begin
                    ShipmentLine.Get(RentalShipmentBuffer2."Document No.", RentalShipmentBuffer2."Line No.", RentalShipmentBuffer2."Entry No.");
                    ShipmentLine.Delete();
                    exit;
                end;
            ShipmentLine.CalcSums(Quantity);
            if ShipmentLine.Quantity <> Line.Quantity then begin
                ShipmentLine.DeleteAll();
                exit;
            end;
        end;
    end;

